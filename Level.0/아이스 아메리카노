class Solution {
    public int[] solution(int money) {
        int[] answer = new int[2];
        answer[0] = money/5500;
        answer[1] = money-answer[0]*5500;
        return answer;
    }
}

// 간단한 풀이
class Solution {
    public int[] solution(int money) {
        return new int[] { money / 5500, money % 5500 };  // 배열에 바로 넣어버리기
    }
}
