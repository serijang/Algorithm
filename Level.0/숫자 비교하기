int solution(int num1, int num2) {
    int answer;
    if(num1 == num2) {
        answer = 1;
    } else {
        answer = -1;
    }
    return answer;
}


# 더 간단한 풀이

class Solution {
    public int solution(int num1, int num2) {
        int answer = (num1 == num2) ? 1 : -1;  // 삼항 연산자 사용!
        return answer;
    }
}
