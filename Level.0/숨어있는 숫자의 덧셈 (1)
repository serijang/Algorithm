class Solution {
    public int solution(String my_string) {
        int answer = 0;
        int cnt = 0;
        for(int i =0; i <= my_string.length()-1; i++) {
            if(my_string.charAt(i) >= 49 && my_string.charAt(i) <= 57) {
                answer += my_string.charAt(i);
                cnt++;
            }
        }
        return answer-(cnt * 48);
    }
}
